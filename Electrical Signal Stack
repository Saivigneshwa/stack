#include <iostream>
using namespace std;

#define SIZE 6

class SignalStack {
    float stack[SIZE];
    int top;

public:
    SignalStack() {
        top = -1;
    }

    void push(float value) {
        if (top == SIZE - 1) {
            cout << "Stack Full! Cannot insert " << value << " V\n";
            return;
        }
        stack[++top] = value;
        cout << "Signal Pushed: " << value << " V\n";
    }

    void pop() {
        if (top == -1) {
            cout << "Stack Empty! No signals to process\n";
            return;
        }
        cout << "Processed Signal: " << stack[top--] << " V\n";
    }

    bool isEmpty() {
        return top == -1;
    }

    bool isFull() {
        return top == SIZE - 1;
    }
};

int main() {
    SignalStack s;

    s.push(1.2);
    s.push(2.5);
    s.push(3.3);
    s.push(4.7);

    cout << "\nReverse Order Processing:\n";
    while (!s.isEmpty()) {
        s.pop();
    }

    s.pop();   // Underflow demonstration

    return 0;
}
